function gameplay_setDialogue()
{
	PlayState.instance.deleteAllDialogue();
	// PlayState.instance.addDialogueJsonPath('fun/gijujaki-gun');
	if (is_debug)
		PlayState.instance.addDialogueJsonPath('scenes/gijujaki/gijujaki-home-attack');
	else
	{
		PlayState.instance.addDialogueJsonPath('start');
	}

	FlxG.save.bind('Diavy-BaseGame', 'Sinco');
	FlxG.save.data.unlocks ??= [];
	FlxG.save.data.endings ??= [];

	ScriptsManager.callScript('generateFunValue');
}
