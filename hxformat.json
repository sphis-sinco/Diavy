{
        "disableFormatting": false,

        "indentation": {
                "tabWidth": 4,
		"character": "tab"
        },
        "lineEnds": {
                "anonFunctionCurly": {
                        "leftCurly": "after",
                        "rightCurly": "both"
                },
                "leftCurly": "both",
                "rightCurly": "both"
        },

        "sameLine": {
                "ifBody": "same",
                "ifElse": "next",
                "doWhile": "next",
                "tryBody": "next",
                "tryCatch": "next"
        },

        "whitespace": {
                "switchPolicy": "around"
        }
}
